{
  "name": "merkur-frontent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev -p 3000",
    "build:next": "next build",
    "start:next": "next start -p ${PORT:-3000}",
    "dev": "NODE_OPTIONS='-r dbc-node-logger' next dev",
    "dev:storybook": "STORYBOOK_ACTIVE=1 storybook dev -p \"${STORYBOOK_PORT:-4000}\"",
    "build:storybook": "STORYBOOK_ACTIVE=1 storybook build -c .storybook -o dist/storybook --quiet && cp -r public/* dist/storybook/",
    "start:storybook": "[ \"$ENABLE_STORYBOOK\" = \"1\" ] && http-server -p \"${STORYBOOK_PORT:-4000}\" dist/storybook --silent || exit 0",
    "cy:run": "npx cypress@13.11.0 run --project ./e2e",
    "cy:open": "npx cypress@13.11.0 open --project ./e2e",
    "build": "next build",
    "start": "NODE_OPTIONS='-r dbc-node-logger' next start",
    "lint": "eslint src/",
    "mock": "scripts/mock-server.sh",
    "postinstall": "npx --yes @dbcdk/npm-tools@latest verify latest"
  },
  "dependencies": {
    "@dbcdk/login-nextjs": "^4.0.9",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.3.6",
    "dbc-node-logger": "^2.0.13",
    "font-awesome": "^4.7.0",
    "next": "^14.2.28",
    "next-auth": "^4.24.11",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@storybook/nextjs": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "@svgr/webpack": "^8.1.0",
    "lazysizes": "^5.3.2",
    "mockserver": "^3.1.1",
    "prettier": "^3.5.3",
    "storybook": "^7.6.20",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "cypress": "13.17.0"
  }
}
