{
  "name": "merkur-frontent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev -p 3000",
    "build:next": "next build",
    "start:next": "next start -p ${PORT:-3000}",
    "dev": "NODE_OPTIONS='-r dbc-node-logger' next dev",
    "dev:storybook": "STORYBOOK_ACTIVE=1 storybook dev -p \"${STORYBOOK_PORT:-4000}\"",
    "build:storybook": "STORYBOOK_ACTIVE=1 storybook build -c .storybook -o dist/storybook --quiet && cp -r public/* dist/storybook/",
    "start:storybook": "[ \"$ENABLE_STORYBOOK\" = \"1\" ] && http-server -p \"${STORYBOOK_PORT:-4000}\" dist/storybook --silent || exit 0",
    "cy:run": "npx cypress@10.3.0 run --project ./e2e",
    "cy:open": "npx cypress@10.3.0 open --project ./e2e",
    "build": "next build",
    "start": "NODE_OPTIONS='-r dbc-node-logger' next start",
    "prettier:format": "prettier --config .prettierrc --write **/*.{md,yml,json,js}",
    "prettier:ci": "prettier **/*.{md,yml,json,js} --check",
    "lint": "eslint src/",
    "mock": "scripts/mock-server.sh",
    "postinstall": "npx --yes @dbcdk/npm-tools@latest verify latest"

  },
  "dependencies": {
    "@dbcdk/login-nextjs": "^3.2.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.0",
    "dbc-node-logger": "^2.0.10",
    "font-awesome": "^4.7.0",
    "next": "^13.1.6",
    "next-auth": "^4.22.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/nextjs": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@svgr/webpack": "^6.2.1",
    "lazysizes": "^5.3.2",
    "mockserver": "^3.1.1",
    "prettier": "^2.4.1",
    "storybook": "^7.0.22",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "cypress": "10.3.0"
  }
}
